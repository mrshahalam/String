public class fifth
{
    public static void reverseTheSentence(String inputString)
    {
        String[] w = inputString.split(" ");
        String o = "";
        for (int i = w.length-1; i >= 0; i--)
        {
            o = o + w[i] + " ";
        }
        System.out.println(o);
    }
    public static void  main(String[]args )
    {
        String inputString = "this is shsha alam";
        reverseTheSentence(inputString);
    }
}
